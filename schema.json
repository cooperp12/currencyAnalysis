            "CurrencyCode"
        ],
        "properties": {
            "Date": {"bsonType": "date"},
            "CurrencyCode": {
                "bsonType": "object",
                "additionalProperties": {
                    "bsonType": "object",
                    "required": [
                        "AUDPerUnit",
                        "UnitsPerAUD"
                    ],
                    "properties": {
                        "AUDPerUnit": {
                            "bsonType": "decimal"
                        },
                        "UnitsPerAUD": {
                            "bsonType": "decimal"
                        }
                    }
                }
            }
        }
    }
}
